{
  "workbench.colorTheme": "Night Owl Light",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.fontSize": 18,
  "editor.tabSize": 2,
  "editor.lineHeight": 50,
  "workbench.startupEditor": "newUntitledFile",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorStyle": "line",
  "editor.insertSpaces": false,
  "prettier.useTabs": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.run": "onSave",
  "editor.fontFamily": "Iosevka, Menlo, Monaco, 'Courier New', monospace",
  "prettier.requireConfig": true,
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.format.enable": false,
  "editor.formatOnSave": true,
  "workbench.preferredDarkColorTheme": "Night Owl",
  "workbench.preferredLightColorTheme": "Night Owl Light",
  "editor.minimap.renderCharacters": false,
  "workbench.activityBar.visible": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdown.preview.scrollPreviewWithEditor": false,
  "markdown.preview.scrollEditorWithPreview": false,
  "banner-comments-plus.font": "Banner",
  "stylelint.reportNeedlessDisables": true,
  "window.autoDetectColorScheme": true,
  "vetur.grammar.customBlocks": {
    "page-query": "graphql",
    "static-query": "graphql"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.external.osxExec": "iTerm.app",
  "eslint.lintTask.options": "",
  "eslint.enable": false,
  "stylelint.enable": false,
  "workbench.statusBar.visible": false,
  "editor.fontLigatures": true,
  "breadcrumbs.filePath": "off",
  "breadcrumbs.symbolPath": "off",
  "editor.wordWrap": "on",
}